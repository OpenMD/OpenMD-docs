	issn = {0021-9606},
	journal = jcp,
	month = {11},
	number = {20},
	pages = {204703},
	title = {{The role of polarizability in the interfacial thermal conductance at the gold--water interface}},
	url = {https://doi.org/10.1063/5.0027847},
	volume = {153},
	year = {2020},
