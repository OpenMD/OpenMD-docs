	keywords = {Rigid macromolecules; Hydrodynamic properties; Bead-shell model; Three-dimensional structure},
	number = {3},
	pages = {265--274},
	title = {Building hydrodynamic bead-shell models for rigid bioparticles of arbitrary shape},
	ty = {JOUR},
